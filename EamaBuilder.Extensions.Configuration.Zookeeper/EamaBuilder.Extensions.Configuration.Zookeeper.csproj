    <PackageReleaseNotes>无</PackageReleaseNotes>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netcoreapp2.0\EamaBuilder.Extensions.Configuration.Zookeeper.xml</DocumentationFile>
