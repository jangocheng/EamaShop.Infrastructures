  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="2.0.0" />
    <PackageReference Include="ZooKeeperNetEx.Recipes" Version="3.4.10.1" />
  </ItemGroup>

</Project>
